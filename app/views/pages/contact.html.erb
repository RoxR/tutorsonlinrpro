<!-- Page Content -->
<div class="container contact">
  <!-- Page Heading/Breadcrumbs -->
  <div class="row">

    <div class="col-lg-12">
      <h2 class="page-header">CONTACT</h2>
    </div>

    <div class="col-lg-3">
      <p>115 Westbourne Terrace<br>London, United Kingdom W2 6QT<br></p>
      <!-- <a href="mailto:name@example.com">info@tutorsonlinepro.com</a> -->
      <%= render partial: 'shared/social' %>
    </div>
  </div>
  <!-- /.row -->
  <!-- Content Row -->
  <div class="row">
    <!-- Map Column -->
    <div class="col-md-12">
      <!-- Embedded Google Map -->
      <br>
      <iframe width="100%" height="400px" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="http://maps.google.com/maps?hl=en&amp;ie=UTF8&amp;ll=51.516793,-0.180319&amp;spn=56.506174,79.013672&amp;t=m&amp;z=4&amp;output=embed"></iframe>
    </div>
  </div>
  <!-- /.row -->
  <!-- Contact Form -->
  <div class="row">
    <div class="col-md-12">
      <h3>SEND US A MESSAGE</h3>


      <form action="https://formspree.io/tutorsonlinepro@gmail.com"
      method="POST">
        <div class="control-group form-group">
          <div class="controls">
            <label>Full Name:</label>
            <input type="text" class="form-control" id="name" required data-validation-required-message="Please enter your name.">
            <p class="help-block"></p>
          </div>
        </div>
        <div class="control-group form-group">
          <div class="controls">
            <label>Phone Number:</label>
            <input type="text" class="form-control" id="phone" required data-validation-required-message="Please enter your phone number.">
          </div>
        </div>
        <div class="control-group form-group">
          <div class="controls">
            <label>Email Address:</label>
            <input type="email" class="form-control" id="email" required data-validation-required-message="Please enter your email address.">
          </div>
        </div>
        <div class="control-group form-group">
          <div class="controls">
            <label>Message:</label>
            <textarea rows="10" cols="100" class="form-control" id="message" required data-validation-required-message="Please enter your message" maxlength="999" style="resize:none"></textarea>
          </div>
        </div>
        <div id="success"></div>
        <!-- For success/fail messages -->
        <button type="submit" class="btn btn-primary">Send Message</button>
      </form>
    </div>
  </div>
  <!-- /.row -->
</div>
<!-- /.container -->


 