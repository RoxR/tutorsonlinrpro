<head>
  <title>Online English lessons</title>
  <link rel="stylesheet" type="text/css" href="css/style.css">
</head>
<body>
  <div id="container">
    <ul class="price-tbl">
      <li class="tbl-block">
        <h3>Basic</h3>
        <div class="price">
          <div class="price-amount">$200</div>
          <div class="price-per">per</div>
          <div class="price-month">month</div>
        </div>
        
        <div class="features-container">
          <div class="features-whats-included">
            What's included...
          </div>
          <ul class="features-list">
            <li><em>2</em> Lessons / week</li>
            <li><em>Private</em> Teacher</li>
            <li><em>Instant</em> Feedback</li>
            <li><em>Video</em> Classes</li>
            <li><em>Consult</em> the teacher</li>
            <li><em>Flexible</em> Hours</li>
          </ul>
        </div>
        <div class="tbl-footer">
          <%= link_to "Book Now", page_path("contact"), class: 'btn btn-secondary' %>
        </div>
      </li>
      <li class="tbl-block">
        <h3>Standard</h3>
        <div class="price">
          <div class="price-amount">$300</div>
          <div class="price-per">per</div>
          <div class="price-month">month</div>
        </div>
        
        <div class="features-container">
          <div class="features-whats-included">
            What's included...
          </div>
          <ul class="features-list">
            <li><em>3</em> Lessons / week</li>
            <li><em>Private</em> Teacher</li>
            <li><em>Instant</em> Feedback</li>
            <li><em>Video</em> Classes</li>
            <li><em>Consult</em> the teacher</li>
            <li><em>email</em> Support</li>
            <li><em>Flexible</em> Hours</li>
          </ul>
        </div>
        <div class="tbl-footer">
          <%= link_to "Book Now", page_path("contact"), class: 'btn btn-secondary' %>
        </div>
      </li>
      <li class="tbl-block">
        <h3>Premium</h3>
        <div class="price">
          <div class="price-amount">$400</div>
          <div class="price-per">per</div>
          <div class="price-month">month</div>
        </div>
        
        <div class="features-container">
          <div class="features-whats-included">
            What's included...
          </div>
